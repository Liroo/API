  },
  "homepage": "https://github.com/Liroo/StreetSpot#readme",
  "dependencies": {
    "app-module-path": "^2.1.0",
    "body-parser": "^1.15.2",
    "command-line-args": "^3.0.1",
    "express": "^4.14.0",
    "helmet": "^2.3.0",
    "mongoose": "^4.6.3",
    "morgan": "^1.7.0",
    "validator": "^6.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.7.1",
    "eslint-config-fbjs": "^1.1.1",
