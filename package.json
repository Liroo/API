{
  "name": "api",
  "version": "0.0.1",
  "description": "api",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "flow": "./node_modules/flow-bin/vendor/flow; test $? -eq 0 -o $? -eq 2",
    "lint": "./node_modules/.bin/eslint src/",
    "precommit": "LIST=`git diff --name-only --cached | grep .*\\.js$`;if [ \"$LIST\" ]; then ./node_modules/.bin/eslint $LIST; fi; yarn test",
