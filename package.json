  },
  "homepage": "https://github.com/Liroo/API#readme",
  "dependencies": {
    "app-module-path": "^2.1.0",
    "body-parser": "^1.15.2",
    "command-line-args": "^3.0.1",
    "connect-mongo": "^1.3.2",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "helmet": "^2.3.0",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.6.4",
    "morgan": "^1.7.0",
    "validator": "^6.0.0"
  },
